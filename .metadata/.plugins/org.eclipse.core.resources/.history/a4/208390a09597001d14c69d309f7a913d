import java.util.Scanner;

public class Exercicio_04 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int n, h;
		double v, sal;
		
		n = sc.nextInt();
		h = sc.nextInt();
		v = sc.nextDouble();
		
		sal = h * v;
		
		System.out.println("NUMBER = " + n);
		System.out.print("SALARY = %.2nf%"+ sal);
		
		
		
		
		
		sc.close();
				
	}

}
