package curso_programacao;

import java.util.Scanner;

public class Estrutura_For_Exercicio01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		
		for(int i=1; i<=x<=1000; i++) {
			if (i % 2 != 0) {
					System.out.println(i);
			}
		}

		sc.close();
	}

}
